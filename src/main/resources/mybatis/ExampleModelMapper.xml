<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rrc.partner_settlement.api.dao.FeeTypeMapper">

    <resultMap id="exampleResultMap" type="com.mood.example.model.ExampleModel">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="city"  property="city" />
        <result column="create_time"  property="createTime" />
        <result column="update_time"  property="updateTime" />
    </resultMap>
    <sql id="base_Column_List">
        id,create_time AS createTime ,update_time AS updateTime,city
    </sql>
    <insert id="insert" parameterType="com.mood.example.model.ExampleModel" useGeneratedKeys="true"
            keyProperty="id">
        INSERT INTO
        example
        <include refid="base_Column_List" />
        VALUES
        (#{id}, #{createTime}, #{updateTime},#{city})
    </insert>

    <select id="query" parameterType="java.util.Map" resultMap="exampleResultMap">
        select
        <include refid="base_Column_List" />
        from example
        WHERE id = #{id}
    </select>
    <select id="queryOne" parameterType="java.util.Map" resultType="com.mood.example.model.ExampleModel">
        select
        <include refid="base_Column_List" />
        from example
        WHERE id = #{id}
    </select>
</mapper>